@model List<Education>
<section class="resume-section" id="education">
    <div class="resume-section-content">
        <h2 class="mb-5">Education</h2>
        @foreach (var education in Model){
            <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
                <div class="flex-grow-1">
                    <h3 class="mb-0">@education.Title</h3>
                    <div class="subheading mb-3">@education.SubTitle</div>
                    <p>GPA: @education.GPA</p>
                </div>
                <div class="flex-shrink-0"><span class="text-primary">@(((DateTime)education.FirstDate).ToString("yyyy")) - @(((DateTime)education.LastDate).ToString("yyyy"))</span></div>
            </div>
        }
    </div>
</section>